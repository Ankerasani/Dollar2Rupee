{
  "version": 2,
  "name": "dollar2rupee-api",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/rates",
      "dest": "/api/rates.js"
    },
    {
      "src": "/api/forex",
      "dest": "/api/forex.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

